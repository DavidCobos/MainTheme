{
  "store.home": { 
    "blocks": [
      "responsive-layout.desktop",
      "responsive-layout.mobile"
    ]
  },

  "responsive-layout.desktop":{
    "children":[
      "flex-layout.row#Hdesktop"
    ]
  },

  "responsive-layout.mobile":{
    "children":[
      "flex-layout.row#Hmobile"
    ]
  },

  "flex-layout.row#Hdesktop":{
    "props":{
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true
    },
    "children":[
      "flex-layout.col#col1"
    ]
  },

  "flex-layout.col#col1":{
    "props":{
      "blockClass":"colHome"
    },
    "children":[
      "stack-layout",
      "tab-layout#D",
      "list-context.product-list#product-slider",
      "rich-text#buscado",
      "rich-text#marcas",
      "slider-layout#Nmarcas",
      "rich-text#nuevo",
      "info-card#HDesktop",
      "info-card#nueva"
    ]
  },

  "flex-layout.row#Hmobile":{
    "props":{
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true
    },
    "children":[
      "flex-layout.col#col2"
    ]
  },

  "flex-layout.col#col2":{
    "props":{
      "blockClass":"colHome"
    },
    "children":[
      "tab-layout#M",
      "list-context.product-list#product-slider",
      "rich-text#buscado",
      "rich-text#marcas",
      "slider-layout#Nmarcas",
      "rich-text#nuevo",
      "info-card#HMobile",
      "info-card#nuevaMobile"
    ]
  },

  //Globales
  
  "stack-layout": {
    "props":{

    },
    "children": ["image#imgL", "flex-layout.row#link"]
  },

  "flex-layout.row#link":{
    "props":{
      "horizontalAlign": "center",
      "verticalAlign": "center"
    },
    "children":[
      "link#visita"
    ]
  },

  "link#visita":{
    "props": {
      "href": "https://www.google.com.mx",
      "label": "Google",
      "displayMode": "button",
      "blockClass": "linkButton"
    }
  },

  "image#imgL":{
    "props": {
      "widt": "100%",
      "height": "400px",
      "src":"https://www.privarsa.com.mx/wp-content/uploads/Conectores-baffles-y-ensambles.jpg",
      "blockClass": "imgFondo"
    }
  },

  "list-context.product-list#product-slider":{
    "blocks": ["product-summary.shelf#producto"],
    "children": ["slider-layout#producto"]
  },

  "product-summary.shelf#producto":{
    "props":{
      "blockClass":"home-product-slider"
    },
    "children":[
      "product-summary-image",
      "product-summary-brand",
      "product-summary-name",
      "product-summary-price",
      "product-summary-buy-button#b1"
    ]
  },

  "product-summary-buy-button#b1":{
    "props":{
      "buyButtonText": "Compra ahora"
    }
  },

  "slider-layout#producto": {
    "props": {
      "itemsPerPage": {
        "desktop": 6,
        "tablet": 3,
        "phone": 1
      },
      "infinite": false,
      "fullwidth": true,
      "showPaginationDots": true,
      "showNavigationArrows": true,
      "blockClass": "product-slider"
    }
  },

  "slider-layout#Nmarcas": {
    "props": {
      "itemsPerPage": {
        "desktop": 5,
        "tablet": 3,
        "phone": 1
      },
      "infinite": true,
