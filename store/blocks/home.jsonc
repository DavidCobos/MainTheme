  //Desktop

  "tab-layout#D":{
    "children":[	
      "tab-list",
      "tab-content"
    ]
  },

  "tab-list":{
    "children":[
      "tab-list.item#1",
      "tab-list.item#2"
    ]
  },

  "tab-content":{
    "children":[
      "tab-content.item#1",
      "tab-content.item#2"
    ]
  },

  "tab-list.item#1":{
    "props":{
      "tabId": "P1",
      "label": "Pestaña 1",
      "defaultActiveTab": true
    }
  },

  "tab-list.item#2":{
    "props":{
      "tabId": "P2",
      "label": "Pestaña 2"
    }
  },

  "tab-content.item#1":{
    "props":{
      "tabId": "P1"
    },
    "children":[
      "video#P1D"
    ]
  },

  "tab-content.item#2":{
    "props":{
      "tabId": "P2"
    },
    "children":[
      "video#P2D"
    ]
  },

  "video#P1D": {
    "props": {
      "width": "100%",
      "height": "600px",
      "loop": false,
      "autoPlay": false,
      "muted": false,
      "src": "https://youtu.be/W-gvMyhGWCU",
      "blockClass": "videoEl"
    }
  },

  "video#P2D": {
    "props": {
      "width": "100%",
      "height": "600px",
      "loop": false,
      "autoPlay": false,
      "muted": false,
      "src": "https://www.youtube.com/watch?v=W4bbToHMHrM",
      "blockClass": "videoEl"
    }
  },

